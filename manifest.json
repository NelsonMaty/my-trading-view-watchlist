{
  "manifest_version": 3,
  "name": "TradingView Watchlist",
  "version": "1.0",
  "description": "Shows a list of tickers in TradingView watchlist",
  "permissions": ["activeTab"],
  "content_scripts": [
    {
      "matches": ["https://www.tradingview.com/"],
      "js": ["content.js"]
    }
  ]
}
