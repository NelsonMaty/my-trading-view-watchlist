{
  "manifest_version": 3,
  "name": "tradingview watchlist",
  "version": "1.0",
  "description": "shows a list of tickers in tradingview watchlist",
  "permissions": [
    "declarativeNetRequest",
    "declarativeNetRequestWithHostAccess"
  ],
  "declarative_net_request": {
    "rule_resources": [
      {
        "id": "rules",
        "path": "rules.json",
        "enabled": true
      }
    ]
  },
  "host_permissions": ["https://www.tradingview.com/*"]
}
